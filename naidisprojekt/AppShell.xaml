<?xml version="1.0" encoding="utf-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    
    xmlns:Pages="clr-namespace:naidisprojekt.Pages"
    x:Class="naidisprojekt.AppShell"
    Shell.TabBarBackgroundColor="White"
    Shell.TabBarForegroundColor="DarkBlue"
    Shell.TabBarUnselectedColor="LightGray">

    <TabBar>
        <ShellContent Icon="homepageiconselected.png"
                      ContentTemplate="{DataTemplate Pages:Homepage}"
                      Route="home"/>
        <ShellContent Icon="favoritenotselected.png"
                      ContentTemplate="{DataTemplate Pages:FavoritesPage}"
                      Route="profile"
                      />
        <ShellContent Icon="profileiconnotselected.png"
              ContentTemplate="{DataTemplate Pages:ProfilePage}"
              Route="profile"/>




    </TabBar>
</Shell>